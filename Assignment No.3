#include <stdio.h>

int main()
{
    int i,n,a[20],j;
    printf("Enter the size of array:\n");
    scanf("%d",&n);
    printf("Enter array elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]>a[j])
            {
            int temp=a[i];
            a[i]=a[j];
            a[j]=temp;
            }
        }
    }
    printf("After sorting the array:\n");
    for(i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }

 return 0;
}
